    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activity.ExampleActivity">

            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>

        </activity>

        <activity android:name=".activity.MainActivity" />

        <activity android:name=".activity.LoginActivity" />

        <activity android:name=".activity.LoginChooseActivity" />

        <activity android:name=".activity.RegisterActivity" />

        <activity android:name=".activity.VegbasketActivity" />

        <activity android:name=".activity.KitchenSearchActivity" />

        <activity android:name=".activity.KitchenListActivity" />

        <activity android:name=".activity.KitchenWelcomeActivity" />

        <activity android:name=".activity.KitchenWatchActivity" />

        <activity android:name=".activity.KitchenRankListActivity" />

        <activity android:name=".activity.KitchenBudegtActivity" />

        <activity android:name=".activity.KitchenCategoryActivity" />

        <activity android:name=".activity.CommentActivity" />

        <activity android:name=".activity.KitChildSearchActivity" />

        <activity android:name=".activity.GuideActivity"/>

    </application>

</manifest>
